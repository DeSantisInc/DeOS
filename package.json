  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-latest": "^6.16.0",
